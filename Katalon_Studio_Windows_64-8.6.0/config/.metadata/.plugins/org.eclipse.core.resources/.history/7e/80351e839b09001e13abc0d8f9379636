int[] test_data = [1,2,3,4,5,6,7,8,9];

ArrayList<Integer> even_numbers = new ArrayList<Integer>() //ArrayList là hàm để lưu trữ một danh sách các số nguyên
ArrayList<Integer> odd_numbers = new ArrayList<Integer>()


for (int i=0; i < test_data.length; i++) {
	
	if(test_data[i] %2 ==0) {
		
		even_numbers.add(test_data[i]);
		
	} else {
		
		odd_numbers.add(test_data[i]);
	}
	
}
System.out.println("Even Numbers:" + even_numbers);
System.out.println("Odd Numbers:" + odd_numbers);